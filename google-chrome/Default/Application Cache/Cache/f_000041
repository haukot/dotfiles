if(dzoffline.verbose)console.log('data folder not exist');if(deleteAllIfNotExist){if(dzoffline.verbose)console.log('delete database');dzDB.deleteAllItems();filesystem.deleteAllFiles();}
return false;});}catch(e){console.log(e.message,e.stack);}},errorHandler:function(e){try{var msg='';switch(e.code){case FileError.QUOTA_EXCEEDED_ERR:msg='QUOTA_EXCEEDED_ERR';break;case FileError.NOT_FOUND_ERR:msg='NOT_FOUND_ERR';break;case FileError.SECURITY_ERR:msg='SECURITY_ERR';break;case FileError.INVALID_MODIFICATION_ERR:msg='INVALID_MODIFICATION_ERR';break;case FileError.INVALID_STATE_ERR:msg='INVALID_STATE_ERR';break;default:msg='Unknown Error';break;};if(dzoffline.verbose)console.log('Error: '+msg);}catch(e){console.log(e.message,e.stack);}}};
