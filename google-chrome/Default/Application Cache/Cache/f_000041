return true;}catch(e){console.log(e.message,e.stack);}},download:function(type,id,songs,data){try{if(dzoffline.verbose)console.log('Download:',type,id,songs);if(!USER.OPTIONS.web_offline){dzoffline.notAvailableAction('premium_only');return false;}
